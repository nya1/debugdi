    "chai": "^4.3.6",
    "inversify": "^6.0.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
